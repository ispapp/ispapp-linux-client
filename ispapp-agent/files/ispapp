# Main settings section
package ispapp
config settings 'settings'
        option enabled '1'
        option login '00000000-0000-0000-0000-000000000000'
        option Domain 'local.longshot-router.com'
        option ListenerPort '443'
        option connected '0'
        option Key ''
        option refreshToken ''
        option accessToken ''
        option updateInterval '1'
        option IperfServer ''
        list pingTargets 'cloud.ispapp.co'
        list pingTargets 'aws-eu-west-2-ping.ispapp.co'
        list pingTargets 'aws-sa-east-1-ping.ispapp.co'
        list pingTargets 'aws-us-east-1-ping.ispapp.co'
        list pingTargets 'aws-us-west-1-ping.ispapp.co'
# Overview settings section
config overview 'overview'
        option enabled '1'
        option highrefresh '0'
        option refreshInterval '0'